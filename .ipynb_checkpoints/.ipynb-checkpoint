    "myCovOrfs={}#dict.fromkeys(len...od so,0)\n",
    "for i in orf_candidates:\n",
    "    if i[2]-i[1]>=covid_threshold: # dali >= ili >\n",
    "        print(i)\n",
    "        s=\"\"\n",
    "        if i[0]==1:\n",
    "            s=record.seq[i[1]:i[2]]\n",
    "            #print(s)\n",
    "        elif i[0]==-1:\n",
    "            s=record.seq[len(record.seq)-i[2]:len(record.seq)-i[1]]\n",
    "            s=s[::-1]\n",
    "            #print(s)\n",
    "            s=str(s)\n",
    "            s=s.replace(\"A\",\"1\")\n",
    "            s=s.replace(\"T\",\"A\")\n",
    "            s=s.replace(\"1\",\"T\")\n",
    "            s=s.replace(\"C\",\"1\")\n",
    "            s=s.replace(\"G\",\"C\")\n",
    "            s=s.replace(\"1\",\"G\")\n",
    "            #print(s)\n",
    "        s=translate_to_protein(s)\n",
    "        myCovOrfs[s]=i\n",
    "for i in myCovOrfs:\n",
    "    print(i)\n",
    "    print(myCovOrfs[i])\n",
    "\"\"\"\n",
